  function displayNextChild(nodeId) 
  {
    
    var node = document.getElementById(nodeId);
    var children = node.getElementsByTagName("img"); //node.childNodes;
    for (var i = 0; i < children.length; i++) 
    {
      
      if (children[i].style.visibility == "visible")
      {
        //alert(children[i]);   
        if (i == children.length - 1 )
          children[0].style.visibility = "visible";
        else
          children[i+1].style.visibility = "visible";
        children[i].style.visibility = "hidden";
        break;
      }
    }
  }
  
  function displayNextVideoChild(nodeId) 
  {
    
    var node = document.getElementById(nodeId);
    var children = node.getElementsByTagName("video"); //node.childNodes;
    for (var i = 0; i < children.length; i++) 
    {
      
      if (children[i].style.visibility == "visible")
      {
        //alert(children[i]);
        if (i == children.length - 1 ){
          children[i].pause();
  
          children[0].play();
          children[0].style.visibility = "visible";
        }else{
          children[i].pause();
  
          children[i+1].play();
          children[i+1].style.visibility = "visible";
        }
        children[i].style.visibility = "hidden";
        break;
      }
    }
  }
  
  var timers = [0,0,0,0];
  
  function start(idx, nodeId, duration) 
  {
    timers[idx] = setInterval(displayNextChild, duration, nodeId);
  }
  
         function myHandler_for_video_in_zone04(e) 
         {
          //if(!e) { e = window.event; }
          //alert("Video Finished in zone 04");
          displayNextVideoChild("zone_04"); 
         }
  
  function stopAll() 
  {
    for (var i = 0 ; i < timers.length ; i++)
    {
       if (timers[i] != 0)
       {
         clearInterval(timers[i]) ;
         timers[i] = 0;
       }
  
    }
    document.getElementById('btnStop').innerHTML="Start Playback";
    document.getElementById('background_01').style.display="none";
    document.getElementById('bodyid').style.overflow="auto";
    document.getElementById('divstopid').style.position="static";
         
  }
  
  function startAll() 
  {
    start(0, 'zone_01', 5000); 
    start(1, 'zone_02', 7000);
    start(2, 'zone_03', 12000);
    start(3, 'zone_04', 17000);
    document.getElementById('btnStop').innerHTML="Stop Playback";
    document.getElementById('background_01').style.display="block";
    document.getElementById('bodyid').style.overflow="hidden";
    document.getElementById('divstopid').style.position="absolute";
  }
  
  function restart() 
  {
    if (document.getElementById('btnStop').innerHTML == "Start Playback")  
      startAll();
    else
      stopAll();
  }


  </script>
  
 
  <script>

    function onChangeUsefullStorage()
    {
      var crtUsefullStorage = document.getElementById("idUrlStorage").value;
      document.getElementById("renderers.file_urlstorage").value = crtUsefullStorage;
    }
    
    function onChangeUsefullPath()
    {
      var crtUsefullPath = document.getElementById("idUrlPath").value;
      document.getElementById("renderers.file_urlpath").value = crtUsefullPath;
    }
    
    function onChangeUsefullFile()
    {
      var crtUsefullFile = document.getElementById("idUrlFile").value;
      document.getElementById("renderers.file_urlname").value = crtUsefullFile;
    }
    
    function onChangeUsefullPath()
    {
      var crtUsefullStorage = document.getElementById("idUrlPath").value;
      document.getElementById("renderers.file_urlpath").value = crtUsefullStorage;
    }
    
    function waitForImageToLoad(imageElement){
      return new Promise(resolve=>{imageElement.onload = resolve})
    }
    
    function waitForFrameToLoad(frameElement){
      return new Promise(resolve=>{frameElement.onload = resolve})
    }
    
    function waitForVideoToLoad(videoElement){
      return new Promise(resolve=>{videoElement.oncanplay = resolve})
    }
    
    var prevMediaType = 0;
    var crtMediaType = 0;
    
    
    function stopPrevMediaType()
    {
      //alert(prevMediaType);
      if (prevMediaType == 0)
        return; 
      if (prevMediaType==1) //Image
      {
        //alert("stop image");
        var myImage = document.getElementById("idImg");
        myImage.src = "";
        myImage.style.display = "none";
      } 
      if (prevMediaType==2) //Video
      {
        //alert("stop video");
        var myVideo = document.getElementById("idVideo");
        //myVideo.stop(); //dont do stop
        myVideo.src="";
        myVideo.style.display = "none";
      } 
      if (prevMediaType==3) //Html
      {
        //alert("stop html");
        var myHtml = document.getElementById("idHtml");
        //myHtml.src=""; //dont do src=""
        myHtml.style.display = "none";
      } 
    }
    
    function stopCrtMediaType()
    {
      //alert(prevMediaType);
      if (crtMediaType == 0)
        return; 
      if (crtMediaType==1) //Image
      {
        //alert("stop image");
        var myImage = document.getElementById("idImg");
        myImage.src = "";
        myImage.style.display = "none";
      } 
      if (crtMediaType==2) //Video
      {
        //alert("stop video");
        var myVideo = document.getElementById("idVideo");
        //myVideo.stop(); //dont do stop
        myVideo.src="";
        myVideo.style.display = "none";
      } 
      if (crtMediaType==3) //Html
      {
        //alert("stop html");
        var myHtml = document.getElementById("idHtml");
        //myHtml.src=""; //dont do src=""
        myHtml.style.display = "none";
      } 
      crtMediaType = 0;
      prevMediaType = 0;
    }

    function loadMediaFile()
    { 
      //alert("loadMediaFile");
      prevMediaType = crtMediaType;
      stopPrevMediaType();
    
      crtMediaType = document.getElementById("idMediaType").value;
      var mediaFileUrlStorage  = document.getElementById("renderers.file_urlstorage").value;
       var mediaFileUrlPath     = document.getElementById("renderers.file_urlpath").value;
       var mediaFileUrlName     = document.getElementById("renderers.file_urlname").value;
       var mediaFileUrlFullName = mediaFileUrlStorage + mediaFileUrlPath + mediaFileUrlName;
      //alert(mediaFileUrlFullName );
    
      if (crtMediaType==1)
      {
       var myImage = document.getElementById("idImg");
       myImage.style.display = "block";
       //var myImage = document.getElementById("z01img02");
       //alert(mediaFileUrlFullName);
       myImage.src = mediaFileUrlFullName;
       waitForImageToLoad(myImage).then(()=>{
         // Image have loaded.
         onChangeDisplayType();
         console.log('Loaded image')
       });
      }
      if (crtMediaType==2)
      {
        var myVideo = document.getElementById("idVideo");
        myVideo.style.display = "block";
        myVideo.src = mediaFileUrlFullName;
        waitForVideoToLoad(myVideo).then(()=>{
         // Video have loaded.
         //onChangeDisplayType();
         myVideo.play();
         console.log('Video can Play');
         //alert("video can play");
       });
      }
    
      if (crtMediaType==3) //HTML
      {
        var myHtml = document.getElementById("idHtml");
        myHtml.style.display = "block";
        myHtml.src = mediaFileUrlFullName;
        //waitForFrameToLoad(myHtml).then(()=>{
         ////onChangeDisplayType();
         //console.log('Html Loaded');
         //alert("Html Loaded");
       //});
      }
    
    }
    
    
    function onChangeImgCanvasSize() 
    {
        var imgDisplayType = document.getElementById("idImgDisplayType");
        var imgCanvasX = document.getElementById("idImgCanvasX");
        var imgCanvasY = document.getElementById("idImgCanvasY");
        var imgDiv = document.getElementById("idImgDiv");
        var img = document.getElementById("idImg");
        //var imgDiv = document.getElementById("z01");
        //var img = document.getElementById("z01img02");
      
        imgDiv.style.width = imgCanvasX.value;
        imgDiv.style.height = imgCanvasY.value;
      
        onChangeDisplayType();
      }
      function onChangeDisplayType() 
      {
        //alert(1);
        
    
        var imgDisplayType = document.getElementById("idImgDisplayType");
        var imgDiv = document.getElementById("idImgDiv");
        //var imgDiv = document.getElementById("z01");
        
        var img = document.getElementById("idImg");
        //var img = document.getElementById("z01img02");
        var imgRealWidth = img.naturalWidth;
        var imgRealHeight = img.naturalHeight;
      
        var imgNewWidth = img.naturalWidth;
        var imgNewHeight = img.naturalHeight;
      
        //var imgCanvasX = document.getElementById("idImgCanvasX");
        //var imgCanvasY = document.getElementById("idImgCanvasY");
      
        var canvasWidth = imgDiv.offsetWidth;
        var canvasHeight = imgDiv.offsetHeight;
      
        var ratioX = 1;
        var ratioY = 1;
        if(canvasWidth > 0)
          ratioX = imgRealWidth/canvasWidth;
        if(canvasHeight > 0)
          ratioY = imgRealHeight/canvasHeight;
      
        if(imgDisplayType.value=="CENTER")
        {
          img.style.width=imgRealWidth+"px";
          img.style.height=imgRealHeight+"px";
          img.style.marginLeft=(canvasWidth-imgRealWidth)/2+"px";
          img.style.marginRight=(canvasWidth-imgRealWidth)/2+"px";
          img.style.marginTop=(canvasHeight-imgRealHeight)/2+"px";
          img.style.marginBottom=(canvasHeight-imgRealHeight)/2+"px";
        }
        if(imgDisplayType.value=="FILL")
        {
          img.style.width="";
          img.style.height="";
          img.style.marginLeft=0;
          img.style.marginRight=0;
          img.style.marginTop=0;
          img.style.marginBottom=0;
      
          if(ratioX < ratioY)
          {
            imgNewWidth = canvasWidth;
            imgNewHeight = imgRealHeight * (imgNewWidth/imgRealWidth);
      
            img.style.width=imgNewWidth+"px";
            img.style.marginTop=(canvasHeight-imgNewHeight)/2+"px";
            img.style.marginBottom=(canvasHeight-imgNewHeight)/2+"px";
          }
          else
          {
            imgNewHeight = canvasHeight;
            imgNewWidth = imgRealWidth * (imgNewHeight/imgRealHeight);
      
            img.style.height=imgNewHeight+"px";
            img.style.marginLeft=(canvasWidth-imgNewWidth)/2+"px";
            img.style.marginRight=(canvasWidth-imgNewWidth)/2+"px";
          }
        }
        if(imgDisplayType.value=="FIT")
        {
          img.style.width="";
          img.style.height="";
          img.style.marginLeft=0;
          img.style.marginRight=0;
          img.style.marginTop=0;
          img.style.marginBottom=0;
          if(ratioX > ratioY)
          {
            imgNewWidth = canvasWidth;
            imgNewHeight = imgRealHeight * (imgNewWidth/imgRealWidth);
            img.style.width=imgNewWidth+"px";
            img.style.marginTop=(canvasHeight-imgNewHeight)/2+"px";
            img.style.marginBottom=(canvasHeight-imgNewHeight)/2+"px";
          }
          else
          {
            imgNewHeight = canvasHeight;
            imgNewWidth = imgRealWidth * (imgNewHeight/imgRealHeight);
            img.style.height=imgNewHeight+"px";
            img.style.marginLeft=(canvasWidth-imgNewWidth)/2+"px";
            img.style.marginRight=(canvasWidth-imgNewWidth)/2+"px";
          }
        }
        if(imgDisplayType.value=="STRETCH")
        {
          img.style.width=canvasWidth+"px";
          img.style.height=canvasHeight+"px";
          img.style.marginLeft=0;
          img.style.marginRight=0;
          img.style.marginTop=0;
          img.style.marginBottom=0;
        }
        if(imgDisplayType.value=="NORMAL")
        {
          img.style.width=imgRealWidth+"px";
          img.style.height=imgRealHeight+"px";
          img.style.marginLeft=0;
          img.style.marginRight=0;
          img.style.marginTop=0;
          img.style.marginBottom=0;
        }
      }

