       function renderingzones_onChangeDesignId()
       {
         //alert("OnChangeDesignId - start");
         var designId = document.getElementById("renderingzones.designId").value;
         //alert(designId);
         var idxDesign = renderingzones_findDesignById(designId);
         if (idxDesign == -1)
           return -1;
         //alert(idxDesign);
         removeComboBoxOptions("renderingzones.zoneId");
         //alert("OnChangeDesignId - removeComboBoxOptions - done");
         //return;
         var combobox = document.getElementById("renderingzones.zoneId");
         var iDesignZone = -1;
         for (iDesignZone = 0; iDesignZone < designs[idxDesign].zones.length; iDesignZone++) 
         {
          var option = document.createElement('option');
          option.text = option.value = designs[idxDesign].zones[iDesignZone].zoneId;
          combobox.add(option);
         }
         //iOldDesign = iCrtDesign;
         iCrtDesign = idxDesign;
         renderingzones_onChangeZoneId();
       }
       
       function renderingzones_onChangeZoneId()
       {
         var inpZoneLeft   = document.getElementById("renderingzones.zoneLeft");
         var inpZoneTop    = document.getElementById("renderingzones.zoneTop");
         var inpZoneWidth  = document.getElementById("renderingzones.zoneWidth");
         var inpZoneHeight = document.getElementById("renderingzones.zoneHeight");
         var inpZoneZOrder = document.getElementById("renderingzones.zoneZOrder");

         var designId = document.getElementById("renderingzones.designId").value;
         var idxDesign = renderingzones_findDesignById(designId);
         if (idxDesign == -1)
           return -1;
         //alert("renderingzones_onChangeZoneId -- idxDesign : " + idxDesign);

         var zoneId = document.getElementById("renderingzones.zoneId").value;
         var idxZone = renderingzones_findZoneById(idxDesign, zoneId);
         if (idxZone == -1)
           return -1;

         //alert("renderingzones_onChangeZoneId -- idxZone : " + idxZone);

         //alert("renderingzones_onChangeZoneId -- idxZone " + designs[idxDesign].zones[idxZone].zoneLeft);

         inpZoneLeft.value   = designs[idxDesign].zones[idxZone].zoneLeft;
         inpZoneTop.value    = designs[idxDesign].zones[idxZone].zoneTop;
         inpZoneWidth.value  = designs[idxDesign].zones[idxZone].zoneWidth;
         inpZoneHeight.value = designs[idxDesign].zones[idxZone].zoneHeight;
         inpZoneZOrder.value = designs[idxDesign].zones[idxZone].zoneZOrder;

         //--------------------
         removeComboBoxOptions("renderingzones.mediaId");
         //alert("OnChangeZoneId - removeComboBoxOptions - done");
         //return;
         var combobox2 = document.getElementById("renderingzones.mediaId");
         var iDesignZoneMedia = -1;
         for (iDesignZoneMedia = 0; iDesignZoneMedia < designs[idxDesign].zones[idxZone].mediaitems.length; iDesignZoneMedia++) 
         {
          //alert("OnChangeZoneId - addComboBoxOptions - option " + iDesignZoneMedia);
          var option2 = document.createElement('option');
          option2.text = option2.value = designs[idxDesign].zones[idxZone].mediaitems[iDesignZoneMedia].mediaId;
          combobox2.add(option2);
         }
         //alert("OnChangeZoneId - addComboBoxOptions - done");
         iCrtZone = idxZone;
         renderingzones_onChangeMediaId();
       }

       //-------------------------------------------------------
       function renderingzones_onChangeMediaId()
       {
         //alert("renderingzones_onChangeZoneId -- start ");           

         var inpMediaId       = document.getElementById("renderingzones.mediaId");
         var inpMediaType     = document.getElementById("renderingzones.mediaType");
         var inpMediaStorage  = document.getElementById("renderingzones.mediaStorage");
         var inpMediaPath     = document.getElementById("renderingzones.mediaPath");
         var inpMediaFile     = document.getElementById("renderingzones.mediaFile");

         var inpImageDisplayType = document.getElementById("renderingzones.imageDisplayType");


         var designId = document.getElementById("renderingzones.designId").value;
         var idxDesign = renderingzones_findDesignById(designId);
         if (idxDesign == -1)
           return -1;
         //alert("renderingzones_onChangeZoneId -- idxDesign : " + idxDesign);

         
         var zoneId = document.getElementById("renderingzones.zoneId").value;
         var idxZone = renderingzones_findZoneById(idxDesign, zoneId);
         if (idxZone == -1)
           return -1;
         //alert("renderingzones_onChangeZoneId -- idxZone : " + idxZone);           

         
         var mediaId  = document.getElementById("renderingzones.mediaId").value;
         //alert(mediaId);
         var idxMedia = renderingzones_findMediaById(idxDesign, idxZone, mediaId);
         if (idxMedia == -1)
           return -1;

         //alert("renderingzones_onChangeZoneId -- idxMedia : " + idxMedia);           

         //inpMediaId.value       = designs[idxDesign].zones[idxZone].mediaItems[idxMediaItem].mediaId;
         inpMediaType.value     = designs[idxDesign].zones[idxZone].mediaitems[idxMedia].mediaType;
         //if (inpMediaStorage.value == "")
         //inpMediaStorage.value  = global_SDKRenderer.getDefaultResourceStorageUrlName();//designs[idxDesign].zones[idxZone].mediaitems[idxMedia].mediaStorage;
         inpMediaStorage.value  = designs[idxDesign].zones[idxZone].mediaitems[idxMedia].mediaStorage;
         inpMediaPath.value     = designs[idxDesign].zones[idxZone].mediaitems[idxMedia].mediaPath;
         inpMediaFile.value     = designs[idxDesign].zones[idxZone].mediaitems[idxMedia].mediaFile;

         if (inpMediaType.value == 1)
           inpImageDisplayType.value = designs[idxDesign].zones[idxZone].mediaitems[idxMedia].imageDisplayType;
         else
           inpImageDisplayType.value = 0 ;

        iCrtMedia = idxMedia;
         //alert("renderingzones_onChangeZoneId -- end " + designs[idxDesign].zones[idxZone].mediaitems[idxMedia].mediaId);           
       }

       //----------------------------
       function removeComboBoxOptions(comboboxid)
       {
         var i;
         //alert("removeComboBoxOptions - start");
         var combobox = document.getElementById(comboboxid);
         //alert(combobox.value);
         combobox.value = "";
         //alert("removeComboBoxOptions - combo  ok" + combobox);
         for(i = combobox.options.length - 1 ; i >= 0 ; i--)
         {
           combobox.remove(i);
         }
       }

       //----------------------
       function renderingzones_findDesignById(designId)
       {
         var i;
         var idxDesign = -1;
         for (i = 0; i < designs.length; i++) 
         {
           if (designs[i].designId == designId)
           {
             idxDesign = i;
             break;
           }
        }
        return idxDesign;
       }

       //---------------------
       function renderingzones_findZoneById(idxDesign, zoneId)
       {
         var i;
         var idxZone = -1;
         for (i = 0; i < designs[idxDesign].zones.length; i++) 
         {
           if (designs[idxDesign].zones[i].zoneId == zoneId)
           {
             idxZone = i;
             break;
           }
        }
        return idxZone;
       }

       //---------------------
       function renderingzones_findMediaById(idxDesign, idxZone, mediaId)
       {
         var i;
         var idxMedia = -1;
         for (i = 0; i < designs[idxDesign].zones[idxZone].mediaitems.length; i++) 
         {
           if (designs[idxDesign].zones[idxZone].mediaitems[i].mediaId == mediaId)
           {
             idxMedia = i;
             break;
           }
        }
        return idxMedia;
       }

       function getMediaTypeName(mnediaTypeId)
       {
         //--------------------- MediaType_None = 0
         if (mediaTypeId == 0)
           return "NONE";
         //--------------------- MediaType_Image = 1
         if (mediaTypeId == 1)
           return "IMAGE";
         //--------------------- MediaType_Video = 2,  
         if (mediaTypeId == 2)
           return "VIDEO";
         //--------------------- MediaType_VideoStream = 3,  
         if (mediaTypeId == 3)
           return "VIDEOSTREAM";
         //--------------------- MediaType_Html = 4,  
         if (mediaTypeId == 4)
           return "HTML";
         //--------------------- MediaType_HtmlTemplate = 5,  
         if (mediaTypeId == 5)
           return "HTMLTEMPLATE";
         //--------------------- MediaType_Rect = 6,  
         if (mediaTypeId == 6)
           return "RECT";
         //--------------------- MediaType_ScreenSaver = 7,  
         if (mediaTypeId == 7)
           return "SCREENSAVER";
         //--------------------- TvHdmiPassThrough = 8,  
         if (mediaTypeId == 8)
           return "TVHDMIPASSTHROUGH";
         //--------------------- Background = 9,  
         if (mediaTypeId == 9)
           return "BACKGROUND";
         //--------------------- Flash = 10,  
         if (mediaTypeId == 10)
           return "FLASH";
      }

      function getMediaTypeId(mnediaTypeName)
       {
         //--------------------- MediaType_None = 0
         if (mediaTypeName == "NONE")
           return 0;
         //--------------------- MediaType_Image = 1
         if (mediaTypeName == "IMAGE")
           return 1;
         //--------------------- MediaType_Video = 2,  
         if (mediaTypeName == "VIDEO")
           return 2;
         //--------------------- MediaType_VideoStream = 3,  
         if (mediaTypeName == "VIDEO_STREAM")
           return 3;
         //--------------------- MediaType_Html = 4,  
         if (mediaTypeName == "HTML")
           return 4;
         //--------------------- MediaType_HtmlTemplate = 5,  
         if (mediaTypeName == "HTML_TEMPLATE")
           return 5;
         //--------------------- MediaType_Rect = 6,  
         if (mediaTypeName == "FILL_RECT")
           return 6;
         //--------------------- MediaType_ScreenSaver = 7,  
         if (mediaTypeName == "SCREN_SAVER")
           return 7;
         //--------------------- TvHdmiPassThrough = 8,  
         if (mediaTypeName == "TV_HDMI_PASS_THROUGH")
           return 8;
         //--------------------- Background = 9,  
         if (mediaTypeName == "BACKGROUND")
           return 9;
         //--------------------- Flash = 10,  
         if (mediaTypeName == "FLASH")
           return 10;
      }


    /*
    MediaType_None         = 0,
    MediaType_Image        = 1,
    MediaType_Video        = 2,
    MediaType_VideoStream  = 3,
    MediaType_Html         = 4,
    MediaType_HtmlTemplate = 5,
    MediaType_Rect         = 6,
    MediaType_ScreenSaver  = 7,
    MediaType_TvHdmiPassThrough = 8,
    MediaType_Background   = 9,
    MediaType_Flash        = 10
  */

