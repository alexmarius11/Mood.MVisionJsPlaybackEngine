    var inpZoneWidth  = document.getElementById("renderingzones.zoneWidth");
    var inpZoneHeight = document.getElementById("renderingzones.zoneHeight");
    var inpZoneZOrder = document.getElementById("renderingzones.zoneZOrder");

    var inpImageDisplayType = document.getElementById("renderingzones.imageDisplayType");


    designs[iDesign].zones[iZone].zoneLeft   = inpZoneLeft.value;
    designs[iDesign].zones[iZone].zoneTop    = inpZoneTop.value;
    designs[iDesign].zones[iZone].zoneWidth  = inpZoneWidth.value;
    designs[iDesign].zones[iZone].zoneHeight = inpZoneHeight.value;
    designs[iDesign].zones[iZone].zoneZOrder = inpZoneZOrder.value;

    designs[iDesign].zones[iZone].mediaitems[iMedia].imageDisplayType = inpImageDisplayType.value;
 

    //alert("renderingzones_prepareAndPlayMediaFile iDesign: " +  iDesign + ", iZone: " + iZone + ", iMedia: " + iMedia + ", designs[0].id: " + designs[0].designId);
    //alert("1");
    var iOld = iOldDesign;
    global_SDKRenderer.changeDesign(iOld, iDesign, designs);//iZone, iMedia, designs);
    iOldDesign = iDesign;
  }


