  <script>

    var timerP = 0;

/**
 * Returns a random integer between min (inclusive) and max (inclusive).
 * The value is no lower than min (or the next integer greater than min
 * if min isn't an integer) and no greater than max (or the next integer
 * lower than max if max isn't an integer).
 * Using Math.round() will give you a non-uniform distribution!
 */
var iter = 0;
var iterDesign = 0;
function getRandomInt(min, max) {
    min = Math.ceil(min);
    max = Math.floor(max);
    return Math.floor(Math.random() * (max - min + 1)) + min;
}
    //------------------------------------
    function delayedPlayback()
    {
      //alert("start");
      iter++ ;
      if (iter == 8)
      {
        iter = 0;
        if (iterDesign == 0){
          iterDesign = 1;
          global_SDKRenderer.changeDesign(0, 1, designs);
        }else{
          iterDesign = 0;  
          global_SDKRenderer.changeDesign(1, 0, designs);
        }
        return;
      }
      if (iterDesign == 0)
      {
        var aiZone = getRandomInt(1,5)-1;
        if (aiZone < 0)
          aiZone = 0;
        if (aiZone > 4)
          aiZone = 4;

        //alert(aiZone);
        var aiMedia = getRandomInt(1,6)-1;
        if (aiMedia < 0)
          aiMedia = 0;
        if (aiZone > 6)
          aiMedia = 6;
        global_SDKRenderer.prepareAndPlayMediaFile(iterDesign, aiZone, aiMedia, designs);
        return;
        //alert(aiMedia);
        //alert(aiZone);
        //alert(aiMedia);
      }
      if (iterDesign == 1)
      {
        var aiZone = getRandomInt(1,3)-1;
        //alert(aiZone);
        var aiMedia = getRandomInt(1,6)-1;
        //alert(aiMedia);
        //alert(aiZone);
        //alert(aiMedia);
        if (aiZone < 0)
          aiZone = 0;
        if (aiZone > 2)
          aiZone = 2;

        //alert(aiZone);
        if (aiMedia < 0)
          aiMedia = 0;
        if (aiMedia > 6)
          aiMedia = 6;
        global_SDKRenderer.prepareAndPlayMediaFile(iterDesign, aiZone, aiMedia, designs);
        return;
      }
      
    }
    document.getElementById('background01').style.display="none";
    //timerP = setInterval(delayedPlayback, 6000);
  
    //---------------------------------------------
    function playback2_stopPlayback() 
    {
      //stopCrtMediaType();
      document.getElementById('btnPlayback2').innerHTML="Show Playback";
      document.getElementById('background01').style.display="none";
      //document.getElementById('bodyid').style.overflow="auto";
      //document.getElementById('divstopid').style.position="static";         
    }

    function playback2_startPlayback() 
    {
      document.getElementById('btnPlayback2').innerHTML="Hide Playback";
      document.getElementById('background01').style.display="block";
      //document.getElementById('bodyid').style.overflow="hidden";
      //document.getElementById('divstopid').style.position="absolute";
    }

    function playback2_restartPlayback() 
    {
      if (document.getElementById('btnPlayback2').innerHTML == "Show Playback")  
        playback2_startPlayback();
      else
        playback2_stopPlayback();
    }

    //---------------------------------------------
    function playback2_stopTimers() 
    {
      document.getElementById('btnTimers2').innerHTML="Start Timers";
      clearInterval(timerP);
    }

    function playback2_startTimers() 
    {
      document.getElementById('btnTimers2').innerHTML="Stop Timers";
      if (iterDesign == 0){
          iterDesign = 0;
          global_SDKRenderer.changeDesign(1, 0, designs);
        }else{
          iterDesign = 1;  
          global_SDKRenderer.changeDesign(0, 1, designs);
      }
      iter = 0;
      timerP = setInterval(delayedPlayback, 6000);
    }

    function playback2_restartTimers() 
    {
      if (document.getElementById('btnTimers2').innerHTML == "Start Timers")  
        playback2_startTimers();
      else
        playback2_stopTimers();
    }
  </script>



